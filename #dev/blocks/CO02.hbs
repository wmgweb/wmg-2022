<div id="block-{{ blockCount }}" class="block block-CO block-CO02">
	<div class="container">
		<div class="block__content">
			<div class="CO02-intro">{{{content.[0].content}}}</div>
			<div class="row">
				{{#each content}}
					{{#if @index includeZero=false}}
						<div class="col-12 col-md-6 col-lg-3">
							<div class="transparent-card">{{{content}}}</div>
						</div>
					{{/if}}
				{{/each}}
			</div>
		</div>
	</div>
</div>
<script>
	jQuery(document).ready(function($) {
		blockFunctions('{{ blockCount }}');
	});
</script>