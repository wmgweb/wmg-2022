<div id="block-{{ blockCount }}" class="block block-SB block-SB04">
	<div class="container">
		<div class="block__content">
			{{#each content}}
				<div class="SB04-block">
					{{{ this.content }}}
				</div>
			{{/each}}
		</div>
	</div>
</div>
<script>
	jQuery(document).ready(function($) {
		blockFunctions('{{ blockCount }}');
		var $block = $('#block-{{ blockCount }}').parent('.wmg-block');

		// Loop through each block list (parents only) and initialise masonry
		$('.SB04-block > ul', $block).each(function() {
			$(this).masonry();
		});
	});
</script>